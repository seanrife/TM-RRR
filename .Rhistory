readInFile <- function(filename) {
df <- read.csv(filename, header = T, stringsAsFactors = F, check.names=F)
names(df) <- gsub(x = names(df),
pattern = "\\]",
replacement = "")
names(df) <- gsub(x = names(df),
pattern = "\\[",
replacement = "_")
return(df)
}
